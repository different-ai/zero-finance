  "private": true,
  "scripts": {
    "build": "pnpm next build",
    "build:remote": "pnpm db:migrate && pnpm next build",
    "build:safe": "pnpm db:migrate:safe && pnpm next build",
    "build:vercel": "node scripts/build-vercel.js",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "dev": "PORT=3050 pnpm next dev --turbopack",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm next start",
    "typecheck": "NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "db:generate": "drizzle-kit generate",
    "db:generate:new": "tsx scripts/generate-migration.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:studio:local": "dotenv -e .env.local -- drizzle-kit studio ",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:migrate:safe": "tsx scripts/migrate.ts || echo 'Migration failed, continuing with build...'",
    "db:migrate:local": "dotenv -e .env.local -- tsx scripts/migrate.ts",
    "db:migrate:test": "dotenv -e .env.test -- tsx scripts/migrate.ts",
    "db:migrate:prod": "dotenv -e .env.prod.local -- tsx scripts/migrate.ts",
    "migrate-from-bank": "node scripts/migrate-from-bank.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "setup:db": "dotenv -e .env.local -- tsx ./scripts/setup-neon-branch.ts"
  },
